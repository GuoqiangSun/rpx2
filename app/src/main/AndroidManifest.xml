<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.o88o.bluetoothrp8">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.o88o.bluetoothrp8.MasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.Rp8MasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.Rp8GTMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.Rp8OEMRMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.Rp8OEMRXPMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.Rp86MMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.BR16PUMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.BR16IRMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.BR16WPMasterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.o88o.bluetoothrp8.SplushActivity"
            android:screenOrientation="portrait">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.o88o.bluetoothrp8.BluetoothService" />


    </application>

</manifest>